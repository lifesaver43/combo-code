{
    "manifest_version": 2,
    "name": "Webshot",
    "description": "Generate Website Mockups Instantly Without Complicated Software",
    "version": "1.0",
    "background": {
        "page": "index.html",
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "./assets/figma.png"
    },
    "icons": {
        "64": "./assets/figma.png"
    },
    "permissions": [
        "tabs"
    ]
}